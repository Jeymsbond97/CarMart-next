{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false, // ← Bu qatorni false ga o'zgartirdim
		"forceConsistentCasingInFileNames": true,
		// "suppressImplicitAnyIndexErrors": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"noImplicitAny": false,  // ← Bu qatorni qo'shing
		"strictNullChecks": false // ← Bu qatorni qo'shing
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
